from ucsmsdk.mometa.lsboot.LsbootPolicy import LsbootPolicy

def conf_mo1(module):
    parent_mo_or_dn = module.params.get('parent_mo_or_dn')
    name = module.params.get('name')
    descr = module.params.get('descr')

    results = {}

    try:
        mo = LsbootPolicy(parent_mo_or_dn="org-root", name="efiboot2", descr="")
        results['success'] = True
    except:
        module.fail_json(msg="login failed")

    return results

def main():
    module = AnsibleModule(
        argument_spec = dict(
            parent_mo_or_dn        = dict(required=True),
            name  = dict(required=True),
            descr  = dict(required=True)
        )
    )

    results = conf_mo1(module)
    module.exit_json(**results)

from ansible.module_utils.basic import *
if __name__ == '__main__':
    main()

